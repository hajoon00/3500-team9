{
  "manifest_version": 3,
  "name": "Night Planner",
  "version": "1.0.0",
  "description": "Generate a perfect night plan based on your starting location",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
} 